package tollbooth;


public class NissanTruck implements Truck {
	private int axles;
	private int weight;
	private final String make = "Nissan";
	public NissanTruck(int axles, int weight) {
		this.axles = axles;
		this.weight = weight;
	}

	@Override
	public int getAxles() {
		return axles;
	}

	@Override
	public int getWeight() {
		return weight;
	}

	@Override
	public String getMake() {
		return make;
	}
}